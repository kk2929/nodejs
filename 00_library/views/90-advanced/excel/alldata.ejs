<thead>
	<tr>
		<th style="width:5%;">ID</th>
		<th style="width:5%;">単元ID</th>
		<th style="width:5%;">生徒ID</th>
		<th style="width:8%;">operation</th>
		<th style="width:8%;">result</th>
		<th style="width:20%;">解答日時</th>
	</tr>
</thead>
<tbody>
	<% for (var i in data) { %>
	<tr>
		<td> <%=data[i].id %></td>
		<td> <%=data[i].course_id %></td>
		<td> <%=data[i].student_id %></td>
		<td> <%=data[i].operation %></td>
		<td> <%=data[i].result %></td>
		<td>
			<!-- 日付がnullだとエラーが出るので処理を分ける -->
			<% if (data[i].created_at !==null) { %>
			<%- data[i].created_at.toFormat('YYYY-MM-DD<br>HH24:MI:SS') %>
			<% } else { %>
			<% } %>
		</td>
	</tr>
	<% } %>
</tbody>